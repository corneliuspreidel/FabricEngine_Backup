use OGLShaderProgram;

operator loadShader(
  io OGLShaderProgram shaderProgram
) {
//  report("loadShader:" + shaderProgram.name);
  shaderProgram.loadShader();
}

operator unloadShader(
  io OGLShaderProgram shaderProgram
) {
//  report("loadShader:" + shaderProgram.name);
  shaderProgram.unloadShader();
}
